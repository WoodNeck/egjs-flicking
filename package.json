{
	"name": "@egjs/flicking",
	"version": "3.4.7-snapshot",
	"description": "Everyday 30 million people experience. It's reliable, flexible and extendable carousel.",
	"main": "dist/flicking.js",
	"module": "dist/flicking.esm.js",
	"sideEffects": false,
	"es2015": "dist/flicking.esm.js",
	"types": "declaration/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "rollup -w --config ./rollup.config.dev.js",
		"build": "rm -rf ./dist ./declaration && rollup -c && npm run declaration && npm run printsizes",
		"declaration": "rm -rf declaration && tsc -p tsconfig.declaration.json",
		"css": "postcss css/*.css --use autoprefixer postcss-clean -d dist/ -m",
		"printsizes": "print-sizes ./dist --exclude=\\.map",
		"test": "karma start",
		"test:chrome": "karma start --chrome",
		"coverage": "karma start --coverage && print-coveralls --sort=desc",
		"lint": "tslint -c tslint.json 'src/**/*.ts'",
		"jsdoc": "rm -rf ./doc && jsdoc -c jsdoc.json",
		"release": "release-helper upstream",
		"coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/naver/egjs-flicking"
	},
	"author": {
		"name": "NAVER Corp."
	},
	"namespace": {
		"eg": "eg"
	},
	"homepage": "https://naver.github.io/egjs-flicking/",
	"bugs": {
		"url": "https://github.com/naver/egjs-flicking/issues"
	},
	"license": "MIT",
	"browserslist": [
		"last 4 version"
	],
	"devDependencies": {
		"@daybrush/jsdoc": "^0.3.7",
		"@egjs/release-helper": "0.0.3",
		"@rollup/plugin-commonjs": "^11.1.0",
		"@rollup/plugin-node-resolve": "^7.1.3",
		"@rollup/plugin-replace": "^2.3.2",
		"@types/karma-chai": "^0.1.1",
		"@types/mocha": "^5.2.5",
		"@types/sinon": "^7.0.3",
		"autoprefixer": "^9.8.5",
		"chai": "^4.2.0",
		"chalk": "^2.4.2",
		"coveralls": "^3.0.2",
		"cpx": "^1.5.0",
		"gh-pages": "^2.0.1",
		"hammer-simulator": "0.0.1",
		"husky": "^1.3.1",
		"karma": "^3.1.4",
		"karma-chai": "^0.1.0",
		"karma-chrome-launcher": "^2.2.0",
		"karma-mocha": "^1.3.0",
		"karma-mocha-reporter": "^2.2.5",
		"karma-sinon": "^1.0.5",
		"karma-typescript-egjs": "^4.0.0",
		"karma-viewport": "^1.0.4",
		"mocha": "^5.2.0",
		"postcss-clean": "^1.1.0",
		"postcss-cli": "^7.1.1",
		"print-coveralls": "^1.2.2",
		"print-sizes": "0.0.3",
		"rollup": "^2.18.2",
		"rollup-plugin-livereload": "^1.3.0",
		"rollup-plugin-postcss": "^3.1.3",
		"rollup-plugin-prototype-minify": "^1.1.0",
		"rollup-plugin-serve": "^1.0.1",
		"rollup-plugin-typescript2": "^0.27.0",
		"rollup-plugin-uglify": "^6.0.4",
		"rollup-plugin-visualizer": "^4.0.4",
		"sinon": "^7.2.3",
		"tslint": "^5.12.1",
		"tslint-consistent-codestyle": "^1.15.0",
		"tslint-eslint-rules": "^5.4.0",
		"tslint-lines-between-class-members": "^1.3.4",
		"typescript": "^3.9.6"
	},
	"dependencies": {},
	"husky": {
		"hooks": {
			"commit-msg": "node config/validate-commit-msg.js",
			"pre-push": "exit 0 || npm run lint"
		}
	}
}
